    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "postcss": "^8.4.24",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.3.0",
